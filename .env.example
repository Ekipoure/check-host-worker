# Agent Configuration
# شناسه یکتا Agent (required - به صورت خودکار توسط سیستم نصب تولید می‌شود)
# نیازی به تنظیم دستی نیست - سیستم به صورت خودکار این مقدار را تنظیم می‌کند
AGENT_ID=

# نام Agent (required)
AGENT_NAME=Check-Host Worker Agent

# موقعیت جغرافیایی (optional)
AGENT_LOCATION=US East
AGENT_COUNTRY_CODE=US
AGENT_COUNTRY=United States
AGENT_CITY=New York

# IP آدرس Agent (optional - اگر خالی باشد از سرور گرفته می‌شود)
AGENT_IP=

# شماره ASN (optional)
AGENT_ASN=

# Server Configuration
# Port برای Agent (default: 8000)
PORT=8000

# Host برای Agent (default: 0.0.0.0)
HOST=0.0.0.0

# Security
# API key برای امنیت (optional - اگر تنظیم شود، تمام درخواست‌ها باید این key را داشته باشند)
# این key باید در وب‌سایت هم تنظیم شود (WORKER_API_KEY)
API_KEY=

# Environment
NODE_ENV=production
